#include <stdio.h>
int main() {
  int a{4}=[1,2,3,4]; //a[4]={1,2,3,4}
  int i = 0;
  for(i=0;i<4;I++)
{
      printf("%d",a[i])
  }
  return 0;
}
EXPECTED OUTPUT:
1234







LEVEL 2
/*Output:
Reversed number = 54321
*/
#include <stdio.h>

int main() {
  int n=12345, reverse = 0; remanider    //int n=12345, reverse = 0, remainder;
  while (n < 0)   //while (n != 0) 
  {
    remainder = n % 10;
    reverse = reverse * 10 + remainder;
    n = n / 10;
  }
  printf("Reversed number = %d", reverse);
  return 0;
}










