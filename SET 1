#include <stdio.h>
/*
output:
prime numbers
enter a number:15
2 3 5 7 11 13
the number is:15 

*/
int main()//int nain()
{
    int f=0; 
    int b,j=0;
    printf("prime numbers:\n");
    printf("enter a number:");
    scanf("%i",&b);  ///scanf("%i",b);
    for(int i=2;i<b;i++)
    {   
        f=0;              
        //for(j=2;j<i;j++)
        for(int j=2;j<i;j++)
       {
        if(i%j==0)
        {
        f=1;
        break;
        }
        
       }
        if(f==0)
        printf("%d ",j);
        
    }
///printf("\nthe number is:%d",b);
printf("the number is:%d"+b);
    return 0;
}


