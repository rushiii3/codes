LEVEL 1
#include <stdio.h>
int main() {
    int  x, y;
    printf("\nInput the first number: "); 
    scanf("%d", &x);
    printf("\nInput the second number: ");
    scan("%d", &y); //scanf("%d", &y);
    if(x > y) 
	{
		int temp;
		temp = x;
		x = y;
		y = temp;
	}
	if((y % x)== 0) 
	{
		printf("\nMultiplied!\n");
	} 
	else 
	{
		printf("\nNot Multiplied!\n") //printf("\nNot Multiplied!\n");
	}
	return 0;
}
OUTPUT : 
Input the first number: 4
Input the second number: 2
Multiplied!








LEVEL 2
#include <stdio.h>
int main() {
	int x, i;
	printf("Input an integer: ");
	scanf("%d", &x), //scanf("%d", &x);
	printf("List of square of each one of the even values from 1 to a %d :\n",x);
	for(i = 2; i <= x; j++) //for(i = 2; i <= x; i++)
	{
		if((i%2) == 0) {
			printf("%d^2 = %d\n", i, i*i);
		}
	}
	return 0;
}
OUTPUT : 
Input an integer: 4
List of square of each one of the even values from 1 to a 4 :
2^2 = 4
4^2 = 16








LEVEL 3
#include<stdio.h>
struct student {
  char grade;
  float marks;
};
int main() {
  int student s1; //struct student s1;
  s1.grade = 'A';
  s1.marks = 97;
  printf("Grade is: %c\n", student.grade);// printf("Grade is: %c\n", s1.grade);
  printf("Marks : %f\n", student.marks);// printf("Marks : %f\n", s1.marks);
  return 0;
}
OUTPUT : 
Grade is: A
Marks : 97


